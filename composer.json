{
    "name": "singularity/yii2-dropzoneinput",
    "description": "Dropzone Input for Yii2 framework",
    "keywords": [
        "singularity",
        "singularity solution",
        "yii",
        "yii2",
        "yii 2",
        "widget",
        "dropzoneinput",
        "dropzone",
        "input"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/singularity-is/yii2-dropzoneinput",
    "authors": [
        {
            "name": "Singularity Solution",
            "email": "hello@singularity.is",
            "homepage": "https://singularity.is",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13",
        "bower-asset/magnific-popup": "dev-master",
        "bower-asset/slick-carousel": "dev-master",
        "bower-asset/jquery-ui-sortable": "dev-master",
        "npm-asset/dropzone": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "singularity\\dropzoneinput\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
